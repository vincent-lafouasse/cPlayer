+ [[ 2 -lt 2 ]]
+ INPUT=./wav/bass.wav
+ OUTDIR=wav/
+ mkdir -p wav/
+ for rate in 8000 16000 22050 44100 48000 96000
++ base
+++ basename ./wav/bass.wav
++ fname=bass.wav
++ echo bass
+ ffmpeg -y -i ./wav/bass.wav -ar 8000 wav//bass_8000Hz.wav
